{
  "name": "@shopbite-de/storefront",
  "version": "1.2.8",
  "main": "nuxt.config.ts",
  "description": "Shopware storefront for food delivery shops",
  "keywords": [
    "shopware",
    "storefront",
    "nuxt",
    "vue",
    "food delivery"
  ],
  "author": "@veliu",
  "license": "MIT",
  "type": "module",
  "scripts": {
    "build": "nuxt build",
    "dev": "nuxt dev",
    "generate": "nuxt generate",
    "preview": "nuxt preview",
    "postinstall": "nuxt prepare",
    "test:unit": "vitest run --coverage",
    "test:ui": "vitest --ui",
    "test:e2e": "playwright test --project=chromium",
    "eslint": "eslint .",
    "eslint:fix": "eslint --fix app/",
    "prettier": "prettier --check \"**/*.{ts,tsx,md,vue}\"",
    "prettier:fix": "prettier --check \"**/*.{ts,tsx,md,vue}\" --write",
    "generate-types": "shopware-api-gen generate --apiType=store",
    "load-schema": "bunx @shopware/api-gen loadSchema --apiType=store",
    "lint:fix": "npm run prettier:fix && npm run eslint:fix"
  },
  "dependencies": {
    "@headlessui/vue": "^1.7.23",
    "@heroicons/vue": "^2.2.0",
    "@iconify-json/lucide": "^1.2.73",
    "@iconify-json/simple-icons": "^1.2.59",
    "@nuxt/content": "^3.8.2",
    "@nuxt/image": "^2.0.0",
    "@nuxt/ui": "^4.1.0",
    "@nuxtjs/plausible": "2.0.1",
    "@nuxtjs/robots": "^5.5.6",
    "@sentry/nuxt": "^10.25.0",
    "@shopware/api-client": "^1.3.0",
    "@shopware/api-gen": "^1.3.3",
    "@shopware/composables": "^1.9.1",
    "@shopware/helpers": "^1.5.0",
    "@shopware/nuxt-module": "^1.4.1",
    "@tailwindcss/vite": "^4.1.17",
    "@unhead/vue": "^2.0.19",
    "@vite-pwa/nuxt": "^1.0.7",
    "@vueuse/core": "^14.0.0",
    "dotenv": "^17.2.3",
    "nuxt": "^4.2.1",
    "uuid": "^13.0.0"
  },
  "devDependencies": {
    "@nuxt/eslint": "^1.10.0",
    "@nuxt/test-utils": "^3.20.1",
    "@playwright/test": "^1.57.0",
    "@types/node": "^25.0.3",
    "@vitejs/plugin-vue": "^6.0.1",
    "@vitest/coverage-v8": "4.0.5",
    "@vitest/ui": "4.0.10",
    "@vue/test-utils": "^2.4.6",
    "dotenv-cli": "^11.0.0",
    "eslint": "^9.39.1",
    "happy-dom": "^20.0.10",
    "jsdom": "^27.2.0",
    "playwright-core": "^1.56.1",
    "prettier": "^3.6.2",
    "typescript": "^5.9.3",
    "vitest": "^4.0.10"
  },
  "packageManager": "pnpm@10.20.0"
}
